/system		ext4		/dev/block/platform/sprd-sdhci.3/by-name/system
/data		ext4		/dev/block/platform/sprd-sdhci.3/by-name/prodinfo4
/cache		ext4		/dev/block/platform/sprd-sdhci.3/by-name/cache
/misc 		emmc 		/dev/block/platform/sprd-sdhci.3/by-name/misc
/boot		emmc		/dev/block/platform/sprd-sdhci.3/by-name/kenel
/recovery	emmc		/dev/block/platform/sprd-sdhci.3/by-name/recovery

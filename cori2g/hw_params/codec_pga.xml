<!-- dolphin PGA configuration file -->
<codec name="dolphin">

<!-- the profile -->
<profile name="speaker">
	<pga name="Master Playback Volume" bit="0" />
	<pga name="Inter PA Playback Volume" bit="4" />
</profile>

<profile name="headphone">
	<pga name="Master Playback Volume" bit="0" />
	<pga name="HeadPhone Playback Volume" bit="4" />
</profile>

<profile name="headphone-spk">
	<pga name="Master Playback Volume" bit="0" />
	<pga name="HeadPhone Playback Volume" bit="4" />
	<pga name="Inter PA Playback Volume" bit="9" />
</profile>

<profile name="earpiece">
	<pga name="Master Playback Volume" bit="0" />
	<pga name="Ear Boost Volume" bit="4" />
</profile>

<profile name="linein-spk">
	<pga name="Line Playback Volume" bit="0" />
	<pga name="Inter PA Playback Volume" bit="4" />
</profile>

<profile name="linein-hp">
	<pga name="Line Playback Volume" bit="0" />
	<pga name="HeadPhone Playback Volume" bit="5" />
</profile>

<profile name="capture">
	<pga name="Capture Volume" bit="0" />
	<pga name="Mic Boost Volume" bit="4" />
</profile>

<profile name="linein-capture">
	<pga name="Capture Volume" bit="0" />
	<pga name="Mic Boost Volume" bit="4" />
	<pga name="LineinRec Capture Volume" bit="5" />
</profile>

<!-- the configuration PGA is the physical vaule -->
<attribute>
<!-- DO NOT modify the mixer attribute -->
	<mixer name="Master Playback Volume">
		<attr max="15" inverse="1" />
	</mixer>

	<mixer name="HeadPhone Playback Volume">
		<attr max="31" inverse="1" />
	</mixer>

	<mixer name="Line Playback Volume">
		<attr max="19" inverse="1" />
	</mixer>

	<mixer name="LineinRec Capture Volume">
		<attr max="7" inverse="0" />
	</mixer>

	<mixer name="Inter PA Playback Volume">
		<attr max="15" inverse="0" />
	</mixer>

	<mixer name="Ear Boost Volume">
		<attr max="1" inverse="0" />
	</mixer>

	<mixer name="Capture Volume">
		<attr max="15" inverse="0" />
	</mixer>

	<mixer name="Mic Boost Volume">
		<attr max="1" inverse="0" />
	</mixer>
</attribute>

</codec>

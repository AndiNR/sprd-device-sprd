ro.sf.lcd_density=160
ro.opengles.version=131072
ro.product.hardware=SPAVD_V1.0.0
ro.tablet.bluetooth.enable=1
ro.tablet.gps.enable=1
ro.tablet.fm.enable=1
#ro.product.resolution=1024x768
ro.callfirewall.disabled=true

debug.hwui.render_dirty_regions=false

# delay for ConnectivityChange broadcast (ms)
conn.connectivity_change_delay = 0

# copy from system.prop for generic sdk
rild.libpath=/system/lib/libreference-ril.so
rild.libargs=-d /dev/ttyS0

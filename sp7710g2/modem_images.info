#filename     file length   download address
#file length and download must begin with "0x"
#the first item MUST be FDL filename
#the last item can be UART device , when set UART device
#file length download address are 0x00

/dev/block/mmcblk0p1	/dev/block/mmcblk0p1	0x3000		0x40000000
/dev/block/mmcblk0p4	/dev/block/mmcblk0p5	0x20000		0x2100000
/dev/block/mmcblk0p3	/dev/block/mmcblk0p3	0x200000	0x20000
/dev/block/mmcblk0p19	/dev/block/mmcblk0p19	0x9f8000	0x400000
/dev/block/mmcblk0p6	/dev/block/mmcblk0p7	0x40000		0x2120000

/dev/ttyS3		/dev/ttyS3		0x00		0x00
